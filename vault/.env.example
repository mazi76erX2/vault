# Azure Search configuration
AZURE_SEARCH_ENDPOINT=https://<your-azure-search-name>.search.windows.net
AZURE_SEARCH_KEY=<your-azure-search-key>

# Azure OpenAI configuration
AZURE_OPENAI_ENDPOINT=https://<your-azure-openai-endpoint>.openai.azure.com/
AZURE_OPENAI_KEY=<your-azure-openai-key>

# Azure AD / MSAL configuration (for authentication)
CLIENT_ID=<your-azure-client-id>
CLIENT_SECRET=<your-azure-client-secret>
TENANT_ID=<your-azure-tenant-id>

# Supabase configuration
SUPABASE_URL=<your-supabase-url>
SUPABASE_KEY=<your-supabase-key>
SUPABASE_JWT=<your-supabase-jwt>

# LDAP configuration for testing
LDAP_SERVER=ldap://<your-ldap-server>:389   # or ldaps://<your-ldap-server>:636
LDAP_USER=cn=<your-ldap-user>,dc=example,dc=com
LDAP_PASSWORD=<your-ldap-password>

# Logging configuration
LOG_LEVEL=INFO     # DEBUG, INFO, WARNING, ERROR
LOG_FILE=backend_logs.log 

DEBUG=True

TESTING=True